		"title": "Google Chrome",
		"arg": "com.google.Chrome",
		$([[ ${browser} == "com.google.Chrome" ]] &amp;&amp; echo $currSelected)
	},
	{
		"title": "Microsoft Edge",
		"arg": "com.microsoft.edgemac",
		$([[ ${browser} == "com.microsoft.edgemac" ]] &amp;&amp; echo $currSelected)
	},
	{
		"title": "Opera",
		"arg": "com.operasoftware.Opera",
		$([[ ${browser} == "com.operasoftware.Opera" ]] &amp;&amp; echo $currSelected)
	},
	{
		"title": "Orion",
		"arg": "com.kagi.kagimacOS",
		$([[ ${browser} == "com.kagi.kagimacOS" ]] &amp;&amp; echo $currSelected)
	},
	{
		"title": "Safari",
		"arg": "com.apple.Safari",
		$([[ ${browser} == "com.apple.Safari" ]] &amp;&amp; echo $currSelected)
	},
	{
		"title": "Zen",
		"arg": "app.zen-browser.zen",
		$([[ ${browser} == "app.zen-browser.zen" ]] &amp;&amp; echo $currSelected)
	}
]}
EOB</string>
