	{
		"title": "System Default",
		"arg": "",
		$([[ -z ${browser} ]] &amp;&amp; echo $currSelected)
	},
	{
		"title": "Arc",
		"arg": "company.thebrowser.Browser",
		$([[ ${browser} == "company.thebrowser.Browser" ]] &amp;&amp; echo $currSelected)
	},
	{
		"title": "Chromium",
		"arg": "org.chromium.Chromium",
		$([[ ${browser} == "org.chromium.Chromium" ]] &amp;&amp; echo $currSelected)
	},
